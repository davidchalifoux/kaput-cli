{
  "name": "kaput-cli",
  "description": "CLI tools for Put.io",
  "version": "1.1.1",
  "author": "David Chalifoux @davidchalifoux",
  "bin": {
    "kaput": "./bin/run"
  },
  "bugs": "https://github.com/davidchalifoux/kaput-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "@putdotio/api-client": "^8.6.1",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.4.9",
    "conf": "^6.2.4",
    "dotenv": "^8.2.0",
    "inquirer": "^7.3.2",
    "moment": "^2.27.0",
    "node-downloader-helper": "^1.0.13"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/davidchalifoux/kaput-cli",
  "keywords": [
    "oclif",
    "put",
    "put.io",
    "kaput",
    "CLI"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "kaput",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "davidchalifoux/kaput-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "pkg . --out-path dist/"
  }
}
